//add two numbers 
function addTwoNumbers(number1, number2) {
  console.log(number1 + number2);
}
addTwoNumbers(3, 4);//OP-7
addTwoNumbers(3, "4");
addTwoNumbers(3, null);
addTwoNumbers(3, a);


//i can store addTwoNumbers inside the variable .


